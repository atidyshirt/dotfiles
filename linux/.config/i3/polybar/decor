[module/sep]
type = custom/text
content = -

content-background = ${xrdb:background}
content-foreground = ${xrdb:background}

[module/dot]
type = custom/text
content = 

content-background = ${xrdb:background}
content-foreground = ${xrdb:color0}
content-padding = 2

;; _-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_

[module/LD]
type = custom/text
content = "%{T3}%{T-}"
content-background = ${xrdb:background}
content-foreground = ${xrdb:color0}

[module/RD]
type = custom/text
content = "%{T3}%{T-}"
